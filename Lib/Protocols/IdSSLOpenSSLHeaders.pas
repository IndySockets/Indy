  Result := False;
  Assert(FFailedLoadList<>nil);

  if (hIdCrypto <> IdNilHandle) and (hIdSSL <> IdNilHandle) and (FFailedLoadList.Count = 0) then begin
    Result := True;
    Exit;
  end;

  FFailedLoadList.Clear;

  if hIdCrypto = IdNilHandle then begin
