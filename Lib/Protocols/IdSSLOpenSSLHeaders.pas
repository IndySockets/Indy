function  ERR_FATAL_ERROR(const l : TIdC_INT) : Boolean;
{$IFDEF USE_INLINE} inline; {$ENDIF}
begin
  Result := (l and ERR_R_FATAL) > 0;
end;

function ERR_GET_REASON(const l : TIdC_INT) : TIdC_INT;
{$IFDEF USE_INLINE} inline; {$ENDIF}
begin
  Result := l and $fff;
end;

